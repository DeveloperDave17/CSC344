#lang racket
( define ( tile background-color c1-color c2-color c3-color )
   ( define tile-background ( square 200 'solid background-color ) )
   ( define r1 (/ 200 2.25) )
   ( define c1 ( circle r1 'solid c1-color ) )
   ( define r2 ( / r1 1.5 ) )
   ( define c2 ( circle r2 'solid c2-color ) )
   ( define r3 ( / r2 1.75 ) )
   ( define c3 ( circle r3 'solid c3-color ) )
   ( overlay c3 c2 c1 tile-background )
)